<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tuoputuo.shootWolf" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>射一射</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="weijieouyang@tuocad.com" href="http://ashooterworld.com">
        Tuoputuo Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/icon/android/horse-36.png" />
        <icon density="mdpi" src="res/icon/android/horse-48.png" />
        <icon density="hdpi" src="res/icon/android/horse-72.png" />
        <icon density="xhdpi" src="res/icon/android/horse-96.png" />
    </platform>
    <preference name="AndroidLaunchMode" value="singleTask" />
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-wechat" spec="^2.1.0">
        <variable name="WECHATAPPID" value="wx5e2f1ef5e4b57376" />
    </plugin>
    <engine name="android" spec="^7.0.0" />
</widget>
